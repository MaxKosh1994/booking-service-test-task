{
  "name": "booking-service",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "//": "=== Development ===",
    "dev": "nodemon --exec ts-node-dev --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "// ": "=== Code Quality ===",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "//  ": "=== Testing ===",
    "test": "NODE_ENV=test jest --runInBand",
    "//   ": "=== Database ===",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo:all",
    "//    ": "=== Documentation ===",
    "docs:generate": "typedoc --out docs src",
    "swagger:gen": "node scripts/generate-swagger.mjs",
    "//     ": "=== Load Testing ===",
    "loadtest:smoke": "k6 run loadtests/smoke.js",
    "loadtest:load": "k6 run loadtests/load.js",
    "loadtest:stress": "k6 run loadtests/stress.js",
    "loadtest:spike": "k6 run loadtests/spike.js",
    "loadtest:soak": "k6 run loadtests/soak.js",
    "loadtest:booking": "k6 run loadtests/booking-scenario.js"
  },
  "dependencies": {
    "express": "^4.21.1",
    "cors": "^2.8.5",
    "cookie-parser": "^1.4.7",
    "morgan": "^1.10.0",
    "http-errors": "^2.0.0",
    "express-rate-limit": "^7.4.0",
    "sequelize": "^6.37.3",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "redis": "^4.7.0",
    "rabbitmq-client": "^4.3.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.2.0",
    "zod": "^3.23.8",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "dotenv": "^16.4.5",
    "fast-csv": "^5.0.5"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "@types/node": "^22.7.5",
    "@types/express": "^4.17.21",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.7",
    "@types/http-errors": "^2.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "ts-node-dev": "^2.0.0",
    "eslint": "^9.13.0",
    "@typescript-eslint/parser": "^8.12.2",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "prettier": "^3.3.3",
    "jest": "^29.7.0",
    "@swc/core": "^1.7.36",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^2.0.16",
    "supertest": "^7.0.0",
    "sequelize-cli": "^6.6.2",
    "husky": "^9.1.6",
    "openapi-types": "^12.1.3",
    "typedoc": "^0.27.5",
    "nodemon": "^3.1.7"
  }
}
